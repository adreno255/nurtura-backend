{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
        "deleteOutDir": true,
        "assets": [
            {
                "include": "generated/**/*",
                "outDir": "dist/src"
            },
            {
                "include": "assets/**/*",
                "outDir": "dist/src"
            }
        ]
    }
}
